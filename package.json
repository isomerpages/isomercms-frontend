{
  "name": "isomercms-frontend",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@atlaskit/tree": "^7.0.2",
    "@braintree/sanitize-url": "^6.0.0",
    "@chakra-ui/react": "^1.8.3",
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^2.8.2",
    "@opengovsg/design-system-react": "0.0.6",
    "@sentry/react": "^5.30.0",
    "@sentry/tracing": "^6.15.0",
    "axios": "^0.21.3",
    "bluebird": "^3.7.1",
    "bootstrap": "^4.6.0",
    "cheerio": "^1.0.0-rc.10",
    "classnames": "^2.3.1",
    "csp-parse": "0.0.2",
    "cypress-file-upload": "^5.0.7",
    "date-fns": "^2.26.0",
    "date-fns-tz": "^1.1.6",
    "dompurify": "^2.3.0",
    "easymde": "^2.16.1",
    "escape-string-regexp": "^4.0.0",
    "framer-motion": "^6.2.6",
    "html-react-parser": "^1.2.7",
    "immutability-helper": "^3.0.1",
    "js-base64": "^3.7.2",
    "libphonenumber-js": "^1.9.48",
    "lodash": "^4.17.21",
    "marked": "^2.1.3",
    "moment-timezone": "^0.5.33",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^12.0.0",
    "react-color": "^2.17.3",
    "react-dom": "^16.14.0",
    "react-hook-form": "^7.17.5",
    "react-icons": "^4.3.1",
    "react-input-mask": "^2.0.4",
    "react-markdown": "^8.0.0",
    "react-query": "^3.34.16",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0",
    "react-simplemde-editor": "^4.1.0",
    "react-toastify": "^6.1.0",
    "react-toggle-button": "^2.2.0",
    "sgds-govtech": "^1.3.16",
    "slugify": "^1.5.3",
    "toml": "^3.0.0",
    "type-fest": "^0.21.3",
    "uuid": "^3.3.3",
    "yaml": "^1.10.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "source .env.development.local && craco start",
    "dev": "source .env && craco start",
    "build": "craco build",
    "release": "npm version $npm_config_isomer_update && git push --tags",
    "test": "craco test",
    "test:ci": "node scripts/run-e2e.js run",
    "test-e2e": "source .env && node scripts/run-e2e.js run",
    "reset-e2e": "source .env && node scripts/reset-e2e.js",
    "lint": "eslint --ext .js --ext .jsx --ignore-path .gitignore .",
    "lint-fix": "eslint --ext .js --ext .jsx --ignore-path .gitignore . --fix",
    "format": "npx prettier --check .",
    "format-fix": "npx prettier --write .",
    "cypress:open": "source .env && node scripts/run-e2e.js open",
    "prepare": "husky install",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.16.3",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@jest/globals": "^27.0.3",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "auto-changelog": "^2.4.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/react-router-dom": "^5.3.3",
    "btoa": "^1.2.1",
    "chromatic": "^6.4.3",
    "cypress": "^7.2.0",
    "cypress-pipe": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "find-webpack": "^2.2.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^4.13.1",
    "pathmodify": "^0.5.0",
    "prettier": "2.2.1",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "storybook-preset-craco": "0.0.6",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.(js|jsx|ts|tsx)": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
